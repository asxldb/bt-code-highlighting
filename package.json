{
  "name": "bt-code-highlighting",
  "displayName": "bt-code-highlighting",
  "description": "This is a code highlighting extension for BT language.",
  "version": "0.0.1",
  "publisher": "asxldb",
  "icon": "icon.png",
  "homepage": "https://bts.wang",
  "repository": {
    "type": "git",
    "url": "https://github.com/asxldb/bt-code-highlighting"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
	"bt",
	"bt-lang",
	"bt-code"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "bt",
        "aliases": [
          "bt",
          "bt"
        ],
        "extensions": [
          ".bt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bt",
        "scopeName": "source.bt",
        "path": "./syntaxes/bt.tmLanguage.json"
      }
    ]
  }
}